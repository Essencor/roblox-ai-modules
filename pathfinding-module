-- MINIMAL PATHFINDING MODULE FOR TESTING
-- This will help us find the exact line causing the error

print("=== PATHFINDING MODULE LOADING ===")
print("Step 1: Module table created")

local PathfindingModule = {}

print("Step 2: Testing basic Lua")
local testNum = 5 + 5
print("Step 2 complete: " .. testNum)

print("Step 3: Testing game access")
if not game then
    error("Game is nil!")
end
print("Step 3 complete")

print("Step 4: Testing game.GetService")
if not game.GetService then
    error("game.GetService is nil!")
end
print("Step 4 complete")

print("Step 5: Loading services")
local success, err = pcall(function()
    local PathfindingService = game:GetService("PathfindingService")
    local Players = game:GetService("Players")
    print("Services loaded:", PathfindingService ~= nil, Players ~= nil)
end)
if not success then
    error("Service loading failed: " .. tostring(err))
end
print("Step 5 complete")

print("Step 6: Testing Vector3")
local testVec = Vector3.new(0, 0, 0)
print("Step 6 complete:", testVec)

print("Step 7: Testing Color3")
local testColor = Color3.new(1, 1, 1)
print("Step 7 complete:", testColor)

print("Step 8: Testing math functions")
local testSin = math.sin(0)
local testCos = math.cos(0)
print("Step 8 complete:", testSin, testCos)

print("Step 9: Testing string functions")
local testStr = "test"
print("Step 9 complete:", testStr)

print("Step 10: Creating config table")
local Config = {
    Test = true,
    Number = 5,
    String = "hello"
}
print("Step 10 complete")

print("Step 11: Testing table operations")
local testTable = {}
table.insert(testTable, "item")
print("Step 11 complete:", #testTable)

print("Step 12: Simple Navigate function")
function PathfindingModule.Navigate(target, mode)
    print("Navigate called with:", target, mode)
    return true
end
print("Step 12 complete")

function PathfindingModule.Stop()
    print("Stop called")
end

function PathfindingModule.GetStatus()
    return {
        isNavigating = false,
        test = true
    }
end

function PathfindingModule.ClearLearning()
    print("ClearLearning called")
end

print("=== MODULE LOADED SUCCESSFULLY ===")
print("Returning PathfindingModule")

return PathfindingModule
